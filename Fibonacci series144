#include <stdio.h>


int fibonacci(int n) {
    if (n == 1) return 0;
    if (n == 2) return 1;

    int a = 0, b = 1, c;
    for (int i = 3; i <= n; i++) {
        c = a + b;
        a = b;
        b = c;
    }
    return b;
}

int main() {
    int n, m;
    scanf("%d %d", &n, &m);


    if (n < 1 || n > 20 || m < 1 || m > 20) {
        printf("Invalid Input\n");
        return 0;
    }

    
    if (n > m) {
        int temp = n;
        n = m;
        m = temp;
    }

  
    int sum = 0;
    for (int i = n; i <= m; i++) {
        sum += fibonacci(i);
    }

  
    printf("The Sum of Fibonacci value is %.1f\n", (float)sum);
    return 0;
}
