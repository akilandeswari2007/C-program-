#include <stdio.h>

struct Calculator {
    int (*square)(int);
};

int findSquare(int n) {
    return n * n;
}

int main() {
    int n;


    scanf("%d", &n);

    if (n >= 1 && n <= 9) {
  
        struct Calculator calc;
        calc.square = findSquare;

  
        int result = calc.square(n);
        printf("Square of %d is %d\n", n, result);
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}
